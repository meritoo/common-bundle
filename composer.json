{
    "name": "meritoo/common-bundle",
    "description": "Common & useful classes, resources, extensions. Based on Symfony framework.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Meritoo.pl",
            "email": "github@meritoo.pl"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-pcre": "*",
        "meritoo/common-library": "^0.1.0",
        "symfony/framework-bundle": "^3.4|^4.1",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "infection/infection": "^0.10.5",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^7.2",
        "sebastian/phpcpd": "^4.0",
        "squizlabs/php_codesniffer": "^2.9",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Meritoo\\CommonBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meritoo\\Test\\CommonBundle\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
