{
    "name": "meritoo/common-bundle",
    "description": "Common & useful classes, resources, extensions. Based on Symfony framework.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Meritoo.pl",
            "email": "github@meritoo.pl",
            "homepage": "http://www.meritoo.pl"
        }
    ],
    "require": {
        "ext-intl": "*",
        "doctrine/annotations": "^1.6",
        "meritoo/common-library": "^1.0",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/validator": "^4.0|^5.0",
        "twig/twig": "^3.2"
    },
    "require-dev": {
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "friendsofphp/php-cs-fixer": "^2.15",
        "infection/infection": "^0.13.1",
        "php-coveralls/php-coveralls": "^2.1",
        "phpspec/phpspec": "^6.0",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^8.1",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/yaml": "^4.1",
        "vimeo/psalm": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Meritoo\\CommonBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meritoo\\Test\\CommonBundle\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
