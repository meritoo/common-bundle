{
    "name": "meritoo/common-bundle",
    "description": "Common & useful classes, resources, extensions. Based on Symfony framework.",
    "type": "library",
    "license": "MIT",
    "version": "0.0.2",
    "authors": [
        {
            "name": "Meritoo.pl",
            "email": "github@meritoo.pl"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-pcre": "*",
        "meritoo/common-library": "^0.1.0",
        "symfony/framework-bundle": "^4.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Meritoo\\CommonBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meritoo\\Test\\CommonBundle\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
